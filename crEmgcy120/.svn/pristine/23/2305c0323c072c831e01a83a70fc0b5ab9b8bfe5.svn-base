<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.emgcy120.core.system.call.mapper.Phone120Mapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.emgcy120.core.system.call.entity.Phone120">
		<id column="id" property="id" />
		<result column="phone_type" property="phoneType" />
		<result column="number" property="number" />
		<result column="ascription" property="ascription" />
		<result column="last_time" property="lastTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, phone_type AS phoneType, number, ascription ,last_time AS lastTime
    </sql>

</mapper>
